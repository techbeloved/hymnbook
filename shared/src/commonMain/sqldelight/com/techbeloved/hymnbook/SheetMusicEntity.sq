import com.techbeloved.hymnbook.shared.model.SheetMusic;

CREATE TABLE SheetMusicEntity (
     id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
     song_id INTEGER,
     file_path TEXT,
     type TEXT AS SheetMusic.Type,
     FOREIGN KEY (song_id) REFERENCES SongEntity (id) ON DELETE CASCADE
);

insert:
INSERT OR REPLACE INTO SheetMusicEntity(song_id, file_path, type)
VALUES (:song_id, :file_path, :type);

lastInsertRowId:
SELECT last_insert_rowid();
